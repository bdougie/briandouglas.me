{
  "name": "@example/basics",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "npm run prebuild && npm run optimize:images && astro build",
    "build:prod": "npm run prebuild && npm run optimize:images:all && astro build",
    "prebuild": "npm run upload:images && npm run replace:paths",
    "preview": "astro preview",
    "astro": "astro",
    "optimize:images": "node scripts/optimize-images.mjs",
    "optimize:images:all": "node scripts/optimize-images.mjs --webp",
    "upload:images": "node scripts/upload-blog-images.mjs",
    "replace:paths": "node scripts/replace-image-paths.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lighthouse:ci": "lhci autorun",
    "lighthouse:collect": "lhci collect --url=http://localhost:4321",
    "lighthouse:assert": "lhci assert",
    "performance:analyze": "npm run build && npm run lighthouse:ci",
    "performance:dashboard": "open http://localhost:4321/performance-dashboard",
    "performance:report": "node scripts/analyze-performance.mjs",
    "performance:full": "npm run build && npm run performance:report && npm run lighthouse:ci"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.3.4",
    "@astrojs/rss": "^4.0.12",
    "@astrojs/tailwind": "^5.1.0",
    "@tailwindcss/typography": "^0.5.10",
    "astro": "^5.2.5",
    "astro-cloudinary": "^1.3.4",
    "cloudinary": "^2.7.0",
    "glob": "^11.0.3",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@astrojs/netlify": "^6.5.13",
    "@astrojs/prefetch": "^0.4.1",
    "@astrojs/sitemap": "^3.5.0",
    "@lhci/cli": "^0.15.1",
    "@netlify/functions": "^5.0.0",
    "@netlify/vite-plugin": "^2.7.3",
    "@squoosh/lib": "^0.3.1",
    "@vitest/ui": "^3.2.4",
    "happy-dom": "^18.0.1",
    "imagemin": "^9.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.1",
    "imagemin-webp": "^8.0.0",
    "playwright": "^1.55.0",
    "sharp": "^0.34.3",
    "sitemap": "^8.0.0",
    "vitest": "^3.2.4",
    "web-vitals": "^5.1.0"
  }
}
